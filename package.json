{
  "name": "goodcache",
  "version": "0.0.2",
  "description": "Some core functions and helper classes. List, Dictionary, Stack, Vec2, Rect, Range2, Tree. Optimized for speed and memory efficiency. Also an object pool a few mixins and Helpers for Dom, Array and Object manipulation + general",
  "main": "./dist/goodcache.umd.min.js",
  "module": "./dist/goodcache.es.2015.js",
  "typings": "./dist/goodcache.d.ts",
  "files": [
    "dist/goodcore.iife.min.js",
    "dist/goodcore.iife.es2015.js",
    "dist/goodcore.umd.min.js",
    "dist/goodcore.es.min.js",
    "dist/goodcore.es.es2015.js",
    "dist/goodcore.d.ts",
    "dist/lib/"
  ],
  "scripts": {
    "install_tools": "yarn install && typings install",
    "clean": "rimraf ./dist",
    "tsc": "tsc",
    "rollup": "node rollup.js",
    "rollup_test": "node rollup_test.js",
    "build": "yarn run clean && tsc && yarn run rollup && yarn run bundle_d_ts",
    "test": "yarn run clean && tsc -p tsconfig-test.json && yarn run_test",
    "run_test": "mocha ./dist/test/*Test.js",
    "cover": "istanbul cover ./node_modules/mocha/bin/_mocha -- ./dist/test/*.js",
    "bundle_d_ts": "node dts-bundle.js",
    "publish_to_npm": "npm publish",
    "all": "yarn run build && yarn run test && yarn run cover"
  },
  "dependencies": {
    "goodcore": "^0.1.13",
    "ts-md5": "^1.2.0"
  },
  "devDependencies": {
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-es2015-classes": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "chai": "^3.5.0",
    "dts-bundle": "^0.7.2",
    "istanbul": "^0.4.5",
    "jsdom": "^9.12.0",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-firefox-launcher": "^1.0.0",
    "karma-mocha": "^1.1.1",
    "karma-mocha-reporter": "^2.1.0",
    "mocha": "^3.0.2",
    "rimraf": "^2.6.1",
    "rollup": "^0.42.0",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-babili": "^3.0.0",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-typescript": "^0.8.1",
    "source-map-support": "^0.4.2",
    "typescript": "^2.3.2",
    "typings": "^2.1.0"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/allnamesrtaken/goodcache.git"
  },
  "keywords": [
    "Typescript",
    "Javascript"
  ],
  "author": "Joel Brage",
  "license": "MIT"
}
